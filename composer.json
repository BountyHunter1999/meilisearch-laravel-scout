{
    "name": "shokme/laravel-scout-meilisearch",
    "description": "Laravel Scout custom engine for MeiliSearch",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Massart",
            "email": "hello@jordan-massart.be"
        }
    ],
    "require": {
        "laravel/scout": "^8.0",
        "meilisearch/meilisearch-php": "^0.9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Shokme\\Meilisearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shokme\\Meilisearch\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shokme\\Meilisearch\\MeilisearchServiceProvider"
            ]
        }
    }
}
